{
  "name": "sufficient",
  "author": "James Smith",
  "version": "1.2.10",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/sufficient",
  "description": "An MVC framework with scheduling.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/sufficient"
  },
  "dependencies": {
    "necessary": "^7.1.3"
  },
  "devDependencies": {
    "@swc/core": "^1.2.50",
    "watchful-cli": "^1.6.8"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "watchful": "watchful -mp=1 --transpiler=swc --source-directory=./src --lib-directory=./lib --pause=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug"
  },
  "main": "./lib/index.js"
}
